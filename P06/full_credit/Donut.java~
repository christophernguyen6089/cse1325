import java.util.ArrayList;

public class Donut extends Product{
    protected Frosting frosting;
    protected boolean sprinkles;
    protected Filling filling;
    
    public void Donut(String name, double price, double cost, Frosting frosting, boolean sprinkles, Filling filling){
        super(name,price,cost);
        this.frosting = frosting;
        this.sprinkles = sprinkles;
        this.filling = filling;
    }
    @Override
    public String toString(){
        //TODO
        return "a";
    }
}

